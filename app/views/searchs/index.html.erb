
<h4 class="card-panel">
  検索 
</h4>

<div class="row">
  <%= form_with(url: "/search/result", method: "get" , class: "col s12", local: true ) do %>
    <div class="row">
      <div class="input-field col s6">
        <%= text_field_tag(:search_word) %>
      </div>
      <div class="input-field col s6">
        <button class="btn waves-effect waves-light" type="submit" name="action">検索
        </button>
      </div>
    </div>
  <% end %>
</div>



